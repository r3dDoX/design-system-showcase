{
  "name": "design-system-showcase",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "customElements": "dist/custom-elements.json",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prebuild": "npm run design-tokens:generate && npm run icons:generate",
    "build": "vite build",
    "storybook": "storybook dev -p 6006",
    "prebuild-storybook": "npm run build",
    "build-storybook": "storybook build",
    "test": "vitest run --reporter=verbose --reporter=junit --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "design-tokens:generate": "node ./style-dictionary.cjs",
    "icons:generate": "node icons.js"
  },
  "dependencies": {
    "@floating-ui/dom": "1.2.7",
    "@tanstack/match-sorter-utils": "8.8.4",
    "@tanstack/table-core": "8.9.1",
    "lit": "2.4.0",
    "lit-html": "2.4.0"
  },
  "devDependencies": {
    "@easepick/core": "1.2.1",
    "@easepick/kbd-plugin": "1.2.1",
    "@faker-js/faker": "7.6.0",
    "@lit-labs/virtualizer": "2.0.1",
    "@open-wc/testing-helpers": "2.2.1",
    "@storybook/addon-actions": "7.0.10",
    "@storybook/addon-essentials": "7.0.10",
    "@storybook/addon-links": "7.0.10",
    "@storybook/addons": "7.0.10",
    "@storybook/theming": "7.0.10",
    "@storybook/web-components": "7.0.10",
    "@storybook/web-components-vite": "7.0.10",
    "@testing-library/dom": "9.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@vitest/coverage-c8": "0.31.0",
    "@web-types/lit": "2.0.0-3",
    "date-fns": "2.30.0",
    "element-internals-polyfill": "1.1.19",
    "eslint": "8.40.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-storybook": "0.6.12",
    "globby": "13.1.4",
    "inter-ui": "3.19.3",
    "jsdom": "22.0.0",
    "modern-css-reset": "1.4.0",
    "pascal-case": "3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shadow-dom-testing-library": "1.10.0",
    "storybook": "7.0.10",
    "storybook-addon-designs": "7.0.0-beta.2",
    "style-dictionary": "3.8.0",
    "ts-lit-plugin": "1.2.1",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vite-plugin-cem": "0.5.0",
    "vite-plugin-dts": "2.3.0",
    "vite-plugin-svgo": "1.3.0",
    "vitest": "0.31.0",
    "yup": "1.1.1"
  }
