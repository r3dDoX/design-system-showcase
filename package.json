{
  "name": "design-system-showcase",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "customElements": "dist/custom-elements.json",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./*"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prebuild": "npm run design-tokens:generate && npm run icons:generate",
    "build": "vite build",
    "storybook": "start-storybook -p 6006",
    "prebuild-storybook": "npm run build",
    "build-storybook": "build-storybook",
    "test": "vitest run --reporter=verbose --reporter=junit --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "design-tokens:generate": "node ./style-dictionary.cjs",
    "icons:generate": "node icons.js"
  },
  "dependencies": {
    "@popperjs/core": "2.11.6",
    "@tanstack/match-sorter-utils": "8.7.2",
    "@tanstack/table-core": "8.7.4",
    "lit": "2.4.0",
    "lit-html": "2.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@easepick/core": "1.2.0",
    "@easepick/kbd-plugin": "1.2.0",
    "@faker-js/faker": "7.6.0",
    "@lit-labs/virtualizer": "1.0.1",
    "@open-wc/testing-helpers": "2.1.4",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/addons": "6.5.15",
    "@storybook/builder-vite": "0.2.6",
    "@storybook/theming": "6.5.15",
    "@storybook/web-components": "6.5.15",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@vitest/coverage-c8": "0.25.8",
    "@web-types/lit": "2.0.0-3",
    "babel-loader": "9.1.0",
    "date-fns": "2.29.3",
    "element-internals-polyfill": "1.1.19",
    "eslint": "8.30.0",
    "eslint-config-standard-with-typescript": "24.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-storybook": "0.6.8",
    "globby": "13.1.3",
    "inter-ui": "3.19.3",
    "jsdom": "21.1.0",
    "modern-css-reset": "1.4.0",
    "pascal-case": "3.1.2",
    "shadow-dom-testing-library": "^1.2.0",
    "storybook-addon-designs": "6.3.1",
    "style-dictionary": "3.7.1",
    "ts-lit-plugin": "1.2.1",
    "typescript": "4.9.4",
    "vite": "4.0.2",
    "vite-plugin-cem": "0.4.0",
    "vite-plugin-dts": "1.7.1",
    "vite-plugin-svgo": "1.2.0",
    "vitest": "0.28.4",
    "yup": "0.32.11"
  }
